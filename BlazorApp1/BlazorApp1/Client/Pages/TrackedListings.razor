@page "/trackedListings"

<PageTitle>Tracked Listings</PageTitle>

<div class="container-fluid">
    <div class="pageTitle">
        <div class="d-flex flex-row">
            <div class="p-2">
                <a href="favourites" class="btn btn-lg btn-outline-primary">Favourites</a>
                <a href="trackedListings" class="btn btn-lg btn-outline-primary">Tracked Listings</a>
            </div>
        </div>
    </div>
    <div class="container listing">
        <div class="row">
            <div class="col-sm-3">
                <img class="img-fixed" src="https://images.pexels.com/photos/1396122/pexels-photo-1396122.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=1">
            </div>
            <div class="col-md">
                <h2>Price: $301,800</h2>
                <h4>Sq ft: 2365</h4>
                <h4>Bed: 3</h4>
                <h4>Bath: 2 + 1</h4>
                <h4>Type: Single Family</h4>
            </div>
            <div class="col">
                 <h2 class="availability"> 
                     <svg xmlns="http://www.w3.org/2000/svg" class="avail icon icon-tabler icon-tabler-circle-check" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                       <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                       <circle cx="12" cy="12" r="9"></circle>
                       <path d="M9 12l2 2l4 -4"></path>
                    </svg>
                    Status: <span class="avail">Available</span>
                </h2> 

                <h3>
                    <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-arrow-big-up-line" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                       <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                       <path d="M9 12h-3.586a1 1 0 0 1 -.707 -1.707l6.586 -6.586a1 1 0 0 1 1.414 0l6.586 6.586a1 1 0 0 1 -.707 1.707h-3.586v6h-6v-6z"></path>
                       <path d="M9 21h6"></path>
                    </svg>
                    Price up $1000 in the past week!
                </h3>
               
                
            </div>
        </div>
    </div>

    <div class="container community">
        <div class="row c">
            <div class="col">
                <h1>Panorama Hills</h1>
                <h4>
                    <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-arrow-big-up-line" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                       <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                       <path d="M9 12h-3.586a1 1 0 0 1 -.707 -1.707l6.586 -6.586a1 1 0 0 1 1.414 0l6.586 6.586a1 1 0 0 1 -.707 1.707h-3.586v6h-6v-6z"></path>
                       <path d="M9 21h6"></path>
                    </svg>
                    Average Listing Price Up $12000</h4>
                <h4>
                    <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-star" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                       <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                       <path d="M12 17.75l-6.172 3.245l1.179 -6.873l-5 -4.867l6.9 -1l3.086 -6.253l3.086 6.253l6.9 1l-5 4.867l1.179 6.873z"></path>
                    </svg>
                    15 New Listings in the Past Week
                </h4>
            </div>
        </div>
    </div>

</div>